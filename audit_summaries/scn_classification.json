     echo \"\ud83d\udcc4 File contents:\"\n+              head -20 \"$EXTERNAL_REPO_FILE\"\n+              exit 1\n+            fi\n+            \n             echo \"\ud83d\udcc4 Processing external_repo_changes.json for public release...\"\n+            echo \"\ud83d\udcc4 Original file size: $(wc -c < \"$EXTERNAL_REPO_FILE\") bytes\"\n+            \n+            # Create output directory if it doesn't exist\n+            mkdir -p \"$(dirname \"$EXTERNAL_REPO_FILE\")\"\n             \n-            # Create sanitized version using jq\n-            jq '{\n-              terraform_changes_detected: .terraform_changes_detected,\n-              total_commits: .total_commits,\n-              infrastructure_impact: .infrastructure_impact,\n-              scn_classification: .scn_classification,\n-              service_impact_analysis: .service_impact_analysis,\n-              repositories_with_changes: (.repositories_with_changes | length),\n-              change_summary: (.change_summary | map(\n-                if type == \"object\" then\n-                  {\n-                    impact_level: .impact_level,\n-                    change_type: .change_type,\n-                    files_changed: (.files_changed | length)\n-                  }\n+            # Create sanitized version with proper null handling\n+            jq --argjson timestamp \"$(date +%s)\" '{\n+              terraform_changes_detected: (.terraform_changes_detected // false),\n+              total_commits: (.total_commits // 0),\n+              infrastructure_impact: (.infrastructure_impact // \"none\"),\n+              scn_classification: (.scn_classification // \"routine_recurring\"),\n+              service_impact_analysis: (.service_impact_analysis // {}),\n+              repositories_with_changes: (\n+                if (.repositories_with_changes != null and (.repositories_with_changes | type) == \"array\") then \n+                  (.repositories_with_changes | length)\n+                else \n+                  0\n+                end\n+              ),\n+              change_summary: (\n+                if (.change_summary != null and (.change_summary | type) == \"array\") then\n+                  (.change_summary | map(\n+                    if (. != null and (. | type) == \"object\") then\n+                      {\n+                        impact_level: (.impact_level // \"unknown\"),\n+                        change_type: (.change_type // \"unknown\"),\n+                        files_changed: (\n+                          if (.files_changed != null and (.files_changed | type) == \"array\") then \n+                            (.files_changed | length)\n+                          else \n+                            0\n+                          end\n+                        )\n+                      }\n+                    else\n+                      \"sanitized_entry\"\n+                    end\n+                  ))\n                 else\n-                  .\n+                  []\n                 end\n-              )),\n+              ),\n               public_summary: \"External repository monitoring data sanitized for public transparency\",\n-              timestamp: (now | todate)\n-            }' \"artifacts/scn-automation-results/external_repo_changes.json\" > \"artifacts/scn-automation-results/external_repo_changes_public.json\"\n+              timestamp: ($timestamp | todate),\n+              sanitization_note: \"Sensitive repository details and commit information removed for security\"\n+            }' \"$EXTERNAL_REPO_FILE\" > \"${EXTERNAL_REPO_FILE%.*}_public.json\"\n             \n-            echo \"\u2705 Created sanitized external_repo_changes_public.json\"\n+            if [ $? -eq 0 ] && [ -f \"${EXTERNAL_REPO_FILE%.*}_public.json\" ]; then\n+              echo \"\u2705 Created sanitized ${EXTERNAL_REPO_FILE%.*}_public.json\"\n+              echo \"\ud83d\udcc4 Sanitized file size: $(wc -c < \"${EXTERNAL_REPO_FILE%.*}_public.jso